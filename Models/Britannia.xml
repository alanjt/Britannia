  <!-- visibility of individual blades, disk and blur dependant upon RPM -->
<animation>
    <type>select</type>
    <object-name>propdisk1</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <property>fdm/jsbsim/propulsion/engine[0]/propeller-rpm</property>
        <value>1100</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>propblur1</object-name>
    <condition>
    <and>
      <greater-than>
        <property>fdm/jsbsim/propulsion/engine[0]/propeller-rpm</property>
        <value>500.000</value>
      </greater-than>
      <less-than>
        <property>fdm/jsbsim/propulsion/engine[0]/propeller-rpm</property>
        <value>1100.001</value>
      </less-than>
    </and>
    </condition>
  </animation>

